                <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="My Assignments" />
                <TableView fx:id="assignmentsTable" prefHeight="406.0" prefWidth="358.0">
                    <columns>
                        <TableColumn fx:id="assignmentNameColumn" minWidth="0.0" prefWidth="188.0000057220459" text="Assignment Name" />
                        <TableColumn fx:id="dueDateColumn" minWidth="0.0" prefWidth="115.20001220703125" text="Due Date" />
                        <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="107.20001220703125" text="Status" />
                    </columns>
                </TableView>
            <StackPane.margin>
               <Insets top="13.0" />
            </StackPane.margin>
            </VBox>
        </StackPane>
    </center>
</BorderPane>
