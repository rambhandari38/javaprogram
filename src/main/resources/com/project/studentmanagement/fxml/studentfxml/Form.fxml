<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.studentmanagement.controllers.studentController.FormController">

    <center>
        <VBox alignment="CENTER" prefHeight="263.0" prefWidth="595.0" spacing="20" style="-fx-padding: 50px;">
            <Label prefHeight="28.0" prefWidth="163.0" style="-fx-font-size: 24px; -fx-font-family: 'Times New Roman';" text="Form Dashboard">
            <font>
               <Font size="38.0" />
            </font></Label>
            <VBox prefHeight="181.0" prefWidth="506.0" spacing="10">
                <Button fx:id="studentFormButton" onAction="#showStudentForm" prefHeight="15.0" prefWidth="195.0" style="-fx-background-color: #964B00; -fx-text-fill: white; -fx-padding: 10px 20px;" text="Student Form">
               <font>
                  <Font size="15.0" />
               </font></Button>
                <Button fx:id="studentProblemFormButton" onAction="#showStudentProblemForm" prefHeight="15.0" prefWidth="195.0" style="-fx-background-color: #964B00; -fx-text-fill: white; -fx-padding: 10px 20px;" text="Student Problem Form">
               <font>
                  <Font size="15.0" />
               </font></Button>
            </VBox>
        </VBox>
    </center>
</BorderPane>
