<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="525.0" prefWidth="665.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.studentmanagement.controllers.studentController.OverviewController">


    <center>
        <StackPane fx:id="contentPane" prefHeight="299.0" prefWidth="537.0" style="-fx-background-color: white; -fx-padding: 20;">
            <VBox spacing="10">
                <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Overview" />
                <TableView fx:id="overviewTable">
                    <columns>
                        <TableColumn fx:id="activityColumn" text="Activity" />
                        <TableColumn fx:id="dateColumn" text="Date" />
                        <TableColumn fx:id="detailsColumn" text="Details" />
                    </columns>
                </TableView>
            </VBox>
        </StackPane>
    </center>
</BorderPane>
