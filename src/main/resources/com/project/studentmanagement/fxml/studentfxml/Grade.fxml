<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="525.0" prefWidth="665.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.studentmanagement.controllers.studentController.GradeController">

    <center>
        <StackPane fx:id="contentPane" prefHeight="299.0" prefWidth="537.0" style="-fx-background-color: white; -fx-padding: 20;">
            <VBox spacing="10">
                <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Grades" />
                <TableView fx:id="gradeTable">
                    <columns>
                        <TableColumn fx:id="subjectColumn" text="Subject" />
                        <TableColumn fx:id="gradeColumn" text="Grade" />
                        <TableColumn fx:id="remarksColumn" text="Remarks" />
                    </columns>
                </TableView>
            </VBox>
        </StackPane>
    </center>
</BorderPane>
